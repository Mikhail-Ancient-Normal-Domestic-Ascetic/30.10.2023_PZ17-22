<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:models="clr-namespace:AvaloniaApplication7.Models"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AvaloniaApplication8.AddTypeEquipment"
        Title="AddTypeEquipment">
        <StackPanel>
                <TextBlock Text="Type"
                ></TextBlock>
                <DataGrid x:Name="GroupsDataGrid1"
                          x:DataType="models:TypeEquipments"
                          AutoGenerateColumns="False"
                          IsReadOnly="True"
                          GridLinesVisibility="All"
                          BorderThickness="1"
                          Margin="0,20"
                          BorderBrush="Gray"
                          Height="100">
                        <DataGrid.Columns>
                                <DataGridTextColumn Header="Название типа"  Binding="{Binding Id}"/>
                                <DataGridTextColumn Header="Название типа"  Binding="{Binding Name}"/>
                        </DataGrid.Columns>
                           
                </DataGrid> 
               
        </StackPanel>
</Window>
